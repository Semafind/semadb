    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "env": {
				"SEMADB_CONFIG": "${workspaceFolder}/config/singleServer.yaml",

    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/server",
            "env": {
                "SEMADB_DEBUG": "true",
                "SEMADB_SERVERS": "localhost:11001",
                "SEMADB_HTTP_PORT": "8081",
                "SEMADB_RPC_PORT": "11001",
                "SEMADB_ROOT_DIR": "./dump"
            }
        },
        {
            "name": "Launch Main",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}"
        }
    ]
}